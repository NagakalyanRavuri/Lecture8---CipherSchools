package corejava;

public class Loops {

	public static void main(String[] args) {
		//FOR LOOP
		for(int i=1;i<=100;i++) {
			System.out.println(i);
		}
		
		//WHILE LOOP
		
		int n = 5;
		while(n>0) {
			System.out.println("Hello world");
			n--;
		}
		
		//DO WHILE LOOP
		int x =5;
		do {
			System.out.println(x);
			x--;
		}
		while(x>0);
	
//do while loop will run once inspite of statement.
	do {
		System.out.println(x);
		x--;
	}while(x>0);
}
}
